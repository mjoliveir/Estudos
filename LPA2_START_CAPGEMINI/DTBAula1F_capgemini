CREATE TABLE PRODUTOS(
	ID INT PRIMARY KEY AUTO_INCREMENT
	NOME
	PREÇO
	QTDESTOQUE
)

CREATE TABLE VENDAS
(
	ID INT PRIMARY KEY AUTO_INCREMENT,
	CLIENTEID INT NOT NULL,
	DATAVENDA DATE NOT NULL, 
	TOTVENDA FLOAT NOT NULL,
	FOREIGN KEY (CLIENTEID) REFERENCES CLIENTES (ID)
)
