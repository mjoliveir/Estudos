Algoritmo "TESTE DA FUNCAO ENQUANTO"

Var
QTDENTREVISTADOS, MAISNOVO, MAISVELHO, MEDIAIDADE, PCTMENORES, IDADEINFORMADA, QTDMAIORES, QTDMENORES, SOMAIDADES: REAL
Inicio
QTDENTREVISTADOS <- 0
MAISNOVO <- 99
MAISVELHO <- 0
QTDMAIORES <- 0
QTDMENORES <- 0
MEDIAIDADE <- 0
SOMAIDADES <- 0
ENQUANTO QTDENTREVISTADOS < 5 FAÇA
         LEIA (IDADEINFORMADA)
              SE IDADEINFORMADA > MAISVELHO ENTAO
                 MAISVELHO <- IDADEINFORMADA
              FIMSE
              SE IDADEINFORMADA < MAISNOVO ENTAO
                 MAISNOVO <- IDADEINFORMADA
              FIMSE
              SE IDADEINFORMADA >= 18 ENTAO
                 QTDMAIORES <- QTDMAIORES + 1
              FIMSE
              SE IDADEINFORMADA < 18 ENTAO
                 QTDMENORES <- QTDMENORES + 1
              FIMSE
              
         QTDENTREVISTADOS <- QTDENTREVISTADOS + 1
         SOMAIDADES <- SOMAIDADES + IDADEINFORMADA
FIMENQUANTO

PCTMENORES <-  QTDMENORES / (QTDMAIORES + QTDMENORES) * 100

ESCREVAL ("ENTREVITAMOS ", QTDENTREVISTADOS, " PESSOAS")
ESCREVAL ("O MAIS NOVO TINHA ", MAISNOVO, " ANOS")
ESCREVAL ("O MAIS VELHO TINHA ", MAISVELHO, " ANOS")
ESCREVAL ("DESSES, ", QTDMAIORES, " ERAM MAIORES DE IDADE")
ESCREVAL ("E OUTROS ", QTDMENORES, " ERAM MENORES")
ESCREVAL ("ESTIMAMOS UMA IDADE MEDIA DE ", MEDIAIDADE, " ANOS")
ESCREVA ("E A PORCENTAGEM DE MENORES FOI DE ", PCTMENORES, "%")

Fimalgoritmo